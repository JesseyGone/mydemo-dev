  <process id="after_sale" name="售后服务流程" isExecutable="true">
    <startEvent id="startevent1" name="Start" activiti:initiator="applyUserId" activiti:formKey="/aftersale/serviceOrder/form"></startEvent>
    <sequenceFlow id="flow1" name="开始-创建服务工单" sourceRef="startevent1" targetRef="usertask2"></sequenceFlow>
    <userTask id="usertask2" name="派工" activiti:assignee="${applyUserId}" activiti:formKey="/aftersale/serviceOrder/toAssign"></userTask>
