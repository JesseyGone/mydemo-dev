    <userTask id="usertask3" name="确认接单(预约时间)" activiti:assignee="${engineer}" activiti:formKey="/aftersale/serviceOrder/toBooking"></userTask>
    <sequenceFlow id="flow4" sourceRef="usertask3" targetRef="exclusivegateway1"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow6" name="否" sourceRef="exclusivegateway1" targetRef="usertask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${flag==0}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow5" name="是" sourceRef="exclusivegateway1" targetRef="usertask4">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${flag==1}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="usertask4" name="现场签到" activiti:assignee="${engineer}"></userTask>
    <sequenceFlow id="flow7" sourceRef="usertask4" targetRef="usertask5"></sequenceFlow>
    <userTask id="usertask5" name="维修完成(结果上传)" activiti:assignee="${engineer}"></userTask>
    <sequenceFlow id="flow8" sourceRef="usertask5" targetRef="usertask6"></sequenceFlow>
    <userTask id="usertask6" name="工单补录" activiti:assignee="${applyUserId}"></userTask>
    <sequenceFlow id="flow9" sourceRef="usertask6" targetRef="usertask7"></sequenceFlow>
    <userTask id="usertask7" name="客户回访" activiti:assignee="${applyUserId}"></userTask>
    <sequenceFlow id="flow10" name="结束" sourceRef="usertask7" targetRef="endevent1"></sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
  </process>
